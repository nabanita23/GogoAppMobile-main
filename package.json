{
  "name": "gogoapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:release": "yarn android:clean && yarn android --variant release",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "ios:release": "yarn ios --configuration Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package && react-native setup-ios-permissions && pod-install"
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency",
    "Calendars",
    "Camera",
    "Contacts",
    "LocationAccuracy",
    "LocationWhenInUse",
    "MediaLibrary",
    "PhotoLibrary",
    "Notifications"
  ],
  "resolutions": {
    "react-native-permissions": "^3.8.0"
  },
  "overrides": {
    "react-native-qrcode-scanner": {
      "react-native-permissions": "^3.8.0"
    }
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.6",
    "@nandorojo/swr-react-native": "^1.2.0",
    "@notifee/react-native": "^7.8.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/dynamic-links": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "dayjs": "^1.11.7",
    "deprecated-react-native-prop-types": "^4.1.0",
    "formik": "^2.2.9",
    "i18n-js": "^4.2.3",
    "lottie-react-native": "^5.1.6",
    "mobx": "^6.9.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react": "^7.6.0",
    "mobx-react-lite": "^3.4.3",
    "mobx-state-tree": "^5.1.8",
    "pod-install": "^0.1.38",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^10.6.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-figma-squircle": "^0.3.3",
